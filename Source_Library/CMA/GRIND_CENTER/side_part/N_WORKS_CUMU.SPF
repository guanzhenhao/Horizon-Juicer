PROC N_WORKS_CUMU DISPLOF
;;**********程序功能**********
;;工件计数
;;***************************

IF INI[103]<>0;0是单件磨削模式,1是批量磨削模式
    INI[98]=INI[98]+1;磨削工件计数
ENDIF

IF (INI[99]<>0) AND (INI[98]>=INI[99]);磨削几件后修整不为零或当前几件大于设定值
    IF INI[98]/INI[99]-TRUNC(INI[98]/INI[99])==0
        INI[100]=1;标记位
    ENDIF
ENDIF

RET

