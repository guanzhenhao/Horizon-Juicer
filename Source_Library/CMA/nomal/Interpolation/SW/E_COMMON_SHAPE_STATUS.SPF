PROC E_COMMON_SHAPE_STATUS(REAL ANG_A,REAL ANG_B,REAL RND_VALUE,VAR REAL STATUS_CACU,VAR REAL LENGTH) DISPLOF
;;**********程序功能**********
;;
;;***************************

IF ANG_A>ANG_B
    LENGTH=RND_VALUE/TAN((180-ANG_A+ANG_B)/2)
    STATUS_CACU=1
ELSE
    LENGTH=RND_VALUE/TAN((180+ANG_A-ANG_B)/2)
    STATUS_CACU=-1
ENDIF

RET

